{
  "name": "reportforge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:auto": "vite --host --open",
    "dev:3001": "vite --port 3001 --host --open",
    "dev:3002": "vite --port 3002 --host --open",
    "dev:any": "vite --port 0 --host --open",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "port:check": "node scripts/port-manager.js check",
    "port:kill": "node scripts/port-manager.js kill",
    "port:find": "node scripts/port-manager.js find",
    "api:dev": "node server/index.cjs",
    "dev:full": "concurrently \"npm run api:dev\" \"npm run dev\""
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@blocknote/core": "^0.31.2",
    "@blocknote/mantine": "^0.31.2",
    "@blocknote/react": "^0.31.2",
    "@blocknote/xl-ai": "^0.31.2",
    "@carbon/react": "^1.84.0",
    "@carbon/styles": "^1.83.0",
    "@ibm/plex": "^6.4.1",
    "@liveblocks/client": "^2.24.3",
    "@liveblocks/node": "^2.24.3",
    "@liveblocks/react": "^2.24.3",
    "@liveblocks/react-blocknote": "^2.24.3",
    "@liveblocks/react-ui": "^2.24.3",
    "@pinecone-database/pinecone": "^6.1.2",
    "@supabase/supabase-js": "^2.50.0",
    "@tiptap/extension-collaboration": "^2.14.0",
    "@tiptap/extension-collaboration-cursor": "^2.14.0",
    "@types/classnames": "^2.3.4",
    "@types/lodash": "^4.17.17",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "ai": "^4.3.16",
    "classnames": "^2.5.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "openai": "^4.104.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "zod": "^3.25.61"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": "20.x"
  }
}
